{
  "name": "grpcnode",
  "version": "2.0.0",
  "description": "When you want a quick gRPC server or client, made with node.",
  "scripts": {
    "build": "microbundle --no-sourcemap -i src/cli.js -o dist --target node -f cjs && chmod +x dist/grpcnode.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/konsumer/grpcnode.git"
  },
  "keywords": [
    "grpc",
    "api",
    "protobuf"
  ],
  "author": "David Konsumer <konsumer@jetboystudio.com> (http://konsumer.js.org/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/konsumer/grpcnode/issues"
  },
  "homepage": "https://github.com/konsumer/grpcnode#readme",
  "dependencies": {
    "@grpc/proto-loader": "^0.5.5",
    "chalk": "^2.4.2",
    "esm": "^3.2.25",
    "grpc": "^1.24.4",
    "indent": "^0.0.2",
    "jsoncolor": "^0.0.2",
    "lodash": "^4.17.20",
    "yargs": "^16.1.1"
  },
  "devDependencies": {
    "microbundle": "^0.12.4"
  },
  "bin": {
    "grpcnode": "dist/grpcnode.js"
  }
